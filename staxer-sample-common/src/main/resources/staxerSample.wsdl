                  name="staxerSampleService" targetNamespace="http://staxer.sample/">
    <wsdl:types>
        <xsd:schema targetNamespace="http://staxer.sample/" elementFormDefault="unqualified">

            <xsd:complexType name="EchoXsdTypes">
