                    <xsd:extension base="tns:XsdTypes"/>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:element name="echoXsdTypes" type="tns:EchoXsdTypes"/>
            <xsd:complexType name="EchoXsdTypesResponse">
